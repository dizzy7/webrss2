{% javascripts
    'bootstrap/js/jquery.js'
    'bootstrap/js/bootstrap.js'
    'js/angularjs/angular.js'
    'js/angularjs/angular-resource.js'
    'js/angularjs/ng-infinite-scroll.min.js'
    'bundles/fosjsrouting/js/router.js'
    output='js/compiled/base.js'
%}
    <script src="{{ asset_url }}" type="text/javascript"></script>
{% endjavascripts %}

<script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}" type="text/javascript"></script>

{% javascripts 'js/libs/*.js' output='js/compiled/libs.js' %}
    <script src="{{ asset_url }}" type="text/javascript"></script>
{% endjavascripts %}

{% javascripts 'js/*.js' output='js/compiled/app.js' %}
    <script src="{{ asset_url }}" type="text/javascript"></script>
{% endjavascripts %}